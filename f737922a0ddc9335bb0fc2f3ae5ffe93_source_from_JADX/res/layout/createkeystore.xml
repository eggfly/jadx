<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="-1" android:layout_height="-1">
    <LinearLayout android:orientation="1" android:padding="7dp" android:layout_width="-1" android:layout_height="-1">
        <TextView android:layout_width="-2" android:layout_height="-2" android:text="File: *" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystorePath" android:layout_width="-1" android:layout_height="-2" android:text="/" style="@g/DialogLabel" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="Alias: *" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystoreAlias" android:layout_width="-1" android:layout_height="-2" android:text="MyAlias" style="@g/DialogEditText" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="Password: *" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystorePassword" android:layout_width="-1" android:layout_height="-2" android:text="" android:inputType="81" style="@g/DialogEditText" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="Confirm Password: *" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystorePassword2" android:layout_width="-1" android:layout_height="-2" android:text="" android:inputType="81" style="@g/DialogEditText" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="Validity (years): *" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystoreValidity" android:layout_width="-1" android:layout_height="-2" android:text="100" style="@g/DialogEditText" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="First and Last Name: *" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystoreName" android:layout_width="-1" android:layout_height="-2" android:text="" style="@g/DialogEditText" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="Organizational Unit:" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystoreOrganizationalUnit" android:layout_width="-1" android:layout_height="-2" android:text="" style="@g/DialogEditText" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="Organization:" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystoreOrganization" android:layout_width="-1" android:layout_height="-2" android:text="" style="@g/DialogEditText" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="City or Locality:" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystoreLocation" android:layout_width="-1" android:layout_height="-2" android:text="" style="@g/DialogEditText" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="State or Province:" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystoreState" android:layout_width="-1" android:layout_height="-2" android:text="" style="@g/DialogEditText" />
        <TextView android:paddingTop="10dp" android:layout_width="-2" android:layout_height="-2" android:text="Country Code:" style="@g/DialogLabel" />
        <EditText android:id="@b/createkeystoreCountry" android:layout_width="-1" android:layout_height="-2" android:text="" style="@g/DialogEditText" />
    </LinearLayout>
</ScrollView>
